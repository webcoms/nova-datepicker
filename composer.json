{
    "name": "webcoms/nova-datepicker",
    "description": "A datepicker field with week numbers for Laravel Nova using flatpickr.",
    "keywords": [
        "laravel",
        "nova",
        "flatpickr",
        "datepicker"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "orchestra/testbench": "^8.3",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Webcoms\\Datepicker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webcoms\\Datepicker\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Webcoms\\Datepicker\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
